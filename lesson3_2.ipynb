    "# BMI值計算公式:    BMI = 體重(公斤) / 身高2(公尺2)\n",
    "\n",
    "height = eval(input(\"請輸入身高(公分):\"))\n",
    "weight = eval(input(\"請輸入體重(公斤):\"))\n",
    "bmi = weight / (height/100) ** 2\n",
    "print('您的身高:',height,'公分',sep='')\n",
    "print('您的體重:',weight,\"公斤\",sep='')\n",
    "print('您的BMI:',bmi,sep='')"
   ]
  }
